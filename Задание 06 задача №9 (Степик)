## // задача №9, сделал через список
var s := Lst(13,11,-12,2,-10,6,2,9,1);
var t := Lst(2,12,12,-2,-10,-5,8,10,13);
var (cnt1,cnt2) := (0,0);
var A: integer := ReadInteger;
for var i := 0 to 8 do
  begin
    if (s[i] > A) or (t[i] > 12)
      then 
        begin
          Print('+'); 
          cnt1 += 1;
        end;
  end;

Println;
Println(cnt1);
